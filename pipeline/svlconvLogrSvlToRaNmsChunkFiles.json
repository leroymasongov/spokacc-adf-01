{
	"name": "svlconvLogrSvlToRaNmsChunkFiles",
	"properties": {
		"activities": [
			{
				"name": "svl-conv-single-file",
				"type": "Custom",
				"dependsOn": [
					{
						"activity": "SetFullFilePath",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": {
						"value": "powershell \"Expand-Archive svlconv.zip -DestinationPath .; ls; .\\cmdSvlConv.ps1; ls\"",
						"type": "Expression"
					},
					"resourceLinkedService": {
						"referenceName": "batchStorage",
						"type": "LinkedServiceReference"
					},
					"folderPath": "svlconv-function-input",
					"referenceObjects": {
						"linkedServices": [],
						"datasets": []
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureBatch4",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "SetTriggerFileName",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "TriggerFileName",
					"value": {
						"value": "@{pipeline().parameters.triggerFilePath}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SetTriggerFolderPath",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "TriggerFolderPath",
					"value": {
						"value": "@{pipeline().parameters.TriggerFolderPath}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SetFullFilePath",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "SetTriggerFileName",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "SetTriggerFolderPath",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "FileFullPath",
					"value": {
						"value": "@concat(variables('TriggerFolderPath'),'/',variables('TriggerFileName'))",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"TriggerFilePath": {
				"type": "string"
			},
			"TriggerFolderPath": {
				"type": "string"
			}
		},
		"variables": {
			"TriggerFileName": {
				"type": "String"
			},
			"ChunkId": {
				"type": "String",
				"defaultValue": "89155"
			},
			"TriggerFolderPath": {
				"type": "String"
			},
			"FileFullPath": {
				"type": "String"
			}
		},
		"annotations": []
	}
}