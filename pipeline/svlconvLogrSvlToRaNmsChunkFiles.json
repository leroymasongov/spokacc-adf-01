{
	"name": "svlconvLogrSvlToRaNmsChunkFiles",
	"properties": {
		"activities": [
			{
				"name": "SetTriggerFileName",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "TriggerFileName",
					"value": {
						"value": "@{pipeline().parameters.triggerFilePath}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SetTriggerFolderPath",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "TriggerFolderPath",
					"value": {
						"value": "@{pipeline().parameters.TriggerFolderPath}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SetFullFilePath",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "SetTriggerFileName",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "SetTriggerFolderPath",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "FileFullPath",
					"value": {
						"value": "@concat(variables('TriggerFolderPath'),'/',variables('TriggerFileName'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SIT svl-conv-single-file",
				"type": "Custom",
				"dependsOn": [
					{
						"activity": "SetFullFilePath",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": {
						"value": "/bin/sh -c '\necho \"ls .\"\nls\npython3 --version\npython3 SvlConv.zip @{variables('TriggerFileName')} -300\n'",
						"type": "Expression"
					},
					"resourceLinkedService": {
						"referenceName": "batchStorage",
						"type": "LinkedServiceReference"
					},
					"folderPath": "svlconv-2",
					"referenceObjects": {
						"linkedServices": [],
						"datasets": []
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureBatchDs",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"triggerFilePath": {
				"type": "string",
				"defaultValue": "L89313.SVL"
			},
			"triggerFolderPath": {
				"type": "string",
				"defaultValue": "nms-files-sync-aws/files/SV_200A_65170"
			}
		},
		"variables": {
			"TriggerFileName": {
				"type": "String"
			},
			"ChunkId": {
				"type": "String",
				"defaultValue": "89155"
			},
			"TriggerFolderPath": {
				"type": "String"
			},
			"FileFullPath": {
				"type": "String"
			}
		},
		"annotations": []
	}
}